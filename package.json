{
  "name": "builtwithelectron",
  "version": "0.0.1",
  "description": "Where awesome electron apps meet",
  "main": "index.js",
  "scripts": {
    "make-dirs": "mkdir -p static/dist/ && mkdir -p static/dist/js static/dist/css static/dist/fonts",
    "copy-fonts": "cp -r node_modules/bootstrap/fonts static/dist && cp -r node_modules/font-awesome/fonts static/dist",
    "build-less": "lessc --clean-css static/src/less/style.less static/dist/css/style.css",
    "build-js": "webpack",
    "build": "npm run make-dirs && npm run copy-fonts && npm run build-less & npm run build-js",
    "watch-less": "watch 'npm run build-less' static/src/less/",
    "watch-js": "webpack --progress --colors --watch",
    "watch": "npm run build && npm run watch-less & npm run watch-js",
    "release-js": "webpack --config webpack.prod.config.js",
    "release": "npm run make-dirs && npm run copy-fonts && npm run build-less & npm run release-js",
    "test": "eslint 'static/src/js/'",
    "serve": "npm run watch & open -a '/Applications/Google Chrome.app' 'http://0.0.0.0:8000'",
    "postinstall": "npm run release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ekonstantinidis/builtwithelectron.git"
  },
  "keywords": [
    "electron",
    "apps",
    "directory",
    "built"
  ],
  "author": "Emmanouil Konstantinidis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ekonstantinidis/builtwithelectron/issues"
  },
  "engines": {
    "node": "4.2.0",
    "npm": "2.14.x"
  },
  "homepage": "https://github.com/ekonstantinidis/builtwithelectron#readme",
  "dependencies": {
    "babel-core": "=6.3.26",
    "babel-eslint": "=4.1.6",
    "babel-loader": "=6.2.0",
    "babel-preset-es2015": "=6.3.13",
    "babel-preset-react": "=6.3.13",
    "bootstrap": "=3.3.6",
    "font-awesome": "=4.5.0",
    "jquery": "=1.11.3",
    "less": "=2.5.3",
    "less-plugin-clean-css": "=1.5.1",
    "malarkey": "=1.3.2",
    "react": "=0.14.0",
    "react-dom": "=0.14.0",
    "react-dropzone": "=3.0.0",
    "react-select": "=0.8.2",
    "reloading": "0.0.6",
    "superagent": "=1.4.0",
    "underscore": "=1.8.3",
    "underscore.string": "=3.2.2",
    "webpack": "=1.12.9"
  },
  "devDependencies": {
    "eslint": "=1.10.3",
    "eslint-plugin-react": "=3.11.3",
    "watch": "=0.16.0"
  }
}
